<% content_for :title, "OnDemand Loop - " + t('.page.title') %>
<div class="container-md content" role="main">
  <%= render partial: '/shared/breadcrumbs', locals: { links: [{text: t('.breadcrumbs.text')}]} %>
  <div class="row">
      <div class="col-md-12">
        <%= render partial: 'actions' %>

        <div data-controller="files-status-refresh">
          <%= render partial: 'files', locals: { files: @files, status: @status } %>
        </div>

      </div>
  </div>
</div>